<!DOCTYPE html>
<html ng-app="koala">
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <link href="~/Content/Stylesheets/frame.css" rel="stylesheet" />
    <link href="~/Content/Stylesheets/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/Stylesheets/toolkit.css" rel="stylesheet" />
    <link href="~/Content/Stylesheets/application.css" rel="stylesheet" />    
    <link href="~/Content/Stylesheets/docs.css" rel="stylesheet" />
    <link href="~/Content/Stylesheets/compoents/message.css" rel="stylesheet" />

    @RenderSection("links", required: false)
</head>

<body class="ang">

    <script src="~/Content/Scripts/jquery.min.js"></script>
    <script src="~/Content/Scripts/angular.min.js"></script>
    <script src="~/Content/Scripts/koala.js"></script>

    <script>
        var app = angular.module("koala", []);
    </script>

    @Html.Partial("_MenuPartial")

    @RenderBody()

    <script src="~/Content/Scripts/Services/AjaxService.js"></script>
    <script src="~/Content/Scripts/toolkit.js"></script>
    <script src="~/Content/Scripts/application.js"></script>
    <script src="~/Content/Scripts/chart.js"></script>

    @RenderSection("scripts", required: false)

</body>
</html>
